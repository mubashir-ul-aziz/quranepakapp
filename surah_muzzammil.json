                {"arabic": "أَشَدُّ", "urdu": "زیادہ"},
                {"arabic": "وَطًأً", "urdu": "مؤثر ہے"}
            ]
        },
        {
            "ayat_no": 7,
            "surah_name": "Surah Al-Muzzammil",
            "arabic": "وَ أَقْوَمُ قِيلًا",
